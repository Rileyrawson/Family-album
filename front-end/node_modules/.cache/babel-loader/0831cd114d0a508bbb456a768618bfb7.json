;AACA,KAHA;AAIA,IAAA,MAJA,oBAIA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,QADA,GACA,IAAA,QAAA,EADA;AAEA,gBAAA,QAAA,CAAA,MAAA,CAAA,OAAA,EAAA,MAAA,CAAA,IAAA,EAAA,MAAA,CAAA,IAAA,CAAA,IAAA,EAAA,MAAA,CAAA,IAAA,CAAA,WAAA,EAAA,MAAA,CAAA,IAAA,CAAA,MAAA;AAFA;AAAA,uBAGA,KAAA,CAAA,IAAA,CAAA,aAAA,EAAA,QAAA,CAHA;;AAAA;AAGA,gBAAA,EAHA;AAAA;AAAA,uBAIA,KAAA,CAAA,IAAA,CAAA,YAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,KADA;AAEA,kBAAA,WAAA,EAAA,MAAA,CAAA,WAFA;AAGA,kBAAA,MAAA,EAAA,MAAA,CAAA,MAHA;AAIA,kBAAA,IAAA,EAAA,EAAA,CAAA,IAAA,CAAA;AAJA,iBAAA,CAJA;;AAAA;AAIA,gBAAA,EAJA;AAUA,gBAAA,MAAA,CAAA,OAAA,GAAA,EAAA,CAAA,IAAA;;AAVA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA,KAfA;AAgBA,IAAA,QAhBA,sBAgBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,KAAA,CAAA,GAAA,CAAA,YAAA,CADA;;AAAA;AACA,gBAAA,QADA;AAEA,gBAAA,MAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA;AAFA,kDAGA,IAHA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA,KApBA;AAqBA,IAAA,UArBA,sBAqBA,IArBA,EAqBA;AACA,WAAA,SAAA,GAAA,EAAA;AACA,WAAA,QAAA,GAAA,IAAA;AACA,KAxBA;AAyBA,IAAA,UAzBA,sBAyBA,IAzBA,EAyBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,KAAA,CAAA,MAAA,CAAA,gBAAA,IAAA,CAAA,GAAA,CADA;;AAAA;AAEA,gBAAA,MAAA,CAAA,QAAA,GAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,QAAA;;AAHA,kDAIA,IAJA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA,KA9BA;AA+BA,IAAA,QA/BA,oBA+BA,IA/BA,EA+BA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,gBAAA,IAAA,CAAA,GAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,QAAA,CAAA,KADA;AAEA,kBAAA,MAAA,EAAA,MAAA,CAAA,QAAA,CAAA,MAFA;AAGA,kBAAA,WAAA,EAAA,MAAA,CAAA,QAAA,CAAA;AAHA,iBAAA,CAFA;;AAAA;AAOA,gBAAA,MAAA,CAAA,QAAA,GAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,QAAA;;AARA,kDASA,IATA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAUA;AAzCA;AAvBA,CAAA","sourcesContent":["<template>\n<div class=\"addFam\">\n\n<center>\n<div class=\"addSection\">\n  <h1>Add Post</h1>\n  <div class=\"heading\">\n      <div class=\"box\">1</div>\n      <h2>Add an Item</h2>\n  </div>\n\n  <div class=\"add\">\n\n      <div class=\"form\">\n        <input v-model=\"title\" placeholder=\"Title\">\n        <p></p>\n        <textarea v-model=\"people\" placeholder=\"People in Photo\"></textarea>\n        <p></p>\n        <textarea v-model=\"description\" placeholder=\"Description\"></textarea>\n        <p></p>\n        <input type=\"file\" name=\"photo\" @change=\"fileChanged\">\n              <p></p>\n        <button @click=\"upload\">Upload</button>\n      </div>\n\n      <div class=\"upload\" v-if=\"addItem\">\n        <h2>{{addItem.title}}</h2>\n        <p>{{addItem.people}}</p>\n        <p>{{addItem.description}}</p>\n        <img :src=\"addItem.path\" />\n      </div>\n    </div>\n</div>\n\n\n<div class=\"editSection\">\n    <h1>Edit Post</h1>\n    <div class=\"heading\">\n      <div class=\"box\">2</div>\n      <h2>Edit/Delete an Item</h2>\n    </div>\n    <div class=\"edit\">\n      <div class=\"form\">\n        <input v-model=\"findTitle\" placeholder=\"Search\">\n        <div class=\"suggestions\" v-if=\"suggestions.length > 0\">\n          <div class=\"suggestion\" v-for=\"s in suggestions\" :key=\"s.id\" @click=\"selectItem(s)\">{{s.title}}\n          </div>\n        </div>\n      </div>\n\n\n      <div class=\"display\">\n      <center>\n        <div class=\"upload\" v-if=\"findItem\">\n            <input v-model=\"findItem.title\">\n          <p></p>\n            <textarea v-model=\"findItem.people\"></textarea>\n          <p></p>\n            <textarea v-model=\"findItem.description\"></textarea>\n          <p></p>\n          <img :src=\"findItem.path\" />\n        </div>\n      </center>\n      </div>\n      <div class=\"actions\" v-if=\"findItem\">\n        <button @click=\"editItem(findItem)\">Edit</button>\n        <button @click=\"deleteItem(findItem)\">Delete</button>\n      </div>\n    </div>\n\n\n</div>\n</center>\n\n</div>\n</template>\n\n\n\n\n\n<script>\nimport axios from 'axios';\nexport default {\n  name: 'Add',\n  data() {\n    return {\n      title: \"\",\n      description: \"\",\n      people: \"\",\n      file: null,\n      addItem: null,\n      items: [],\n      findTitle: \"\",\n      findItem: null,\n    }\n  },\n  computed: {\n     suggestions() {\n       let items = this.items.filter(item => item.title.toLowerCase().startsWith(this.findTitle.toLowerCase()));\n       return items.sort((a, b) => a.title > b.title);\n     }\n   },\n  created() {\n    this.getItems();\n  },\n  methods: {\n    fileChanged(event) {\n      this.file = event.target.files[0]\n    },\n    async upload() {\n        const formData = new FormData();\n        formData.append('photo', this.file, this.file.name, this.file.description, this.file.people)\n        let r1 = await axios.post('/api/photos', formData);\n        let r2 = await axios.post('/api/items', {\n          title: this.title,\n          description: this.description,\n          people: this.people,\n          path: r1.data.path\n        });\n        this.addItem = r2.data;\n    },\n    async getItems() {\n        let response = await axios.get(\"/api/items\");\n        this.items = response.data;\n        return true;\n    },\n    selectItem(item) {\n      this.findTitle = \"\";\n      this.findItem = item;\n   },\n   async deleteItem(item) {\n       await axios.delete(\"/api/items/\" + item._id);\n       this.findItem = null;\n       this.getItems();\n       return true;\n   },\n   async editItem(item) {\n\n        await axios.put(\"/api/items/\" + item._id, {\n          title: this.findItem.title,\n          people: this.findItem.people,\n          description: this.findItem.description,\n        });\n        this.findItem = null;\n        this.getItems();\n        return true;\n    },\n  },\n}\n</script>\n\n\n\n\n\n\n\n<style scoped>\n.image h2 {\n  font-style: italic;\n  font-size: 1em;\n}\n\n.image p {\n  font-size: .5em;\n}\n\n.heading {\n  display: flex;\n  margin-bottom: 20px;\n  margin-top: 20px;\n}\n\n.heading h2 {\n  margin-top: 8px;\n  margin-left: 10px;\n}\n\n.add,\n.edit, {\n  display: flex;\n  justify-content: center;\n  align-content:center;\n}\n\n.display{\n  padding: 10px;\n}\n\n.addSection,\n.editSection {\n  background: #1c3738;\n  margin: 60px;\n  padding: 30px;\n  color: #fff;\n  width: 50%;\n}\n\n.box {\n  width: 18px;\n  height: 18px;\n  padding: 8px;\n  background: #84a9c0;\n  color: #fff;\n  text-align: center;\n}\n\n\n/* Form */\ninput,\ntextarea,\nselect,\nbutton,\n.description {\n  font-family: 'Montserrat', sans-serif;\n  font-size: 1em;\n}\n\n.description {\n  font-size: .5em;\n}\n\n.form {\n  margin-right: 50px;\n}\n\n/* Uploaded images */\n.upload h2 ,\n.upload p {\n  margin: 0px;\n}\n\n.upload img {\n  max-width: 300px;\n}\n\n\n\n\n/* Suggestions */\n.suggestions {\n  width: 200px;\n  border: 1px solid #ccc;\n}\n\n.suggestion {\n  min-height: 20px;\n}\n\n.suggestion:hover {\n  background-color: #5BDEFF;\n  color: #fff;\n}\n\n\n\n\n\n\n\n\n</style>\n"],"sourceRoot":"src/views"}]}