,GAAA,CAAA,CAAA,QAAA;AAAA,OAAA,CAAA;AACA;AAJA,GAdA;AAoBA,EAAA,OApBA,qBAoBA;AACA,SAAA,SAAA;AACA,GAtBA;AAuBA,EAAA,OAAA,EAAA;AACA,IAAA,WADA,uBACA,KADA,EACA;AACA,WAAA,UAAA,GAAA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,CAAA,CAAA;AACA,KAHA;AAIA,IAAA,MAJA,oBAIA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,QADA,GACA,IAAA,QAAA,EADA;AAEA,gBAAA,QAAA,CAAA,MAAA,CAAA,OAAA,EAAA,MAAA,CAAA,UAAA,EAAA,MAAA,CAAA,UAAA,CAAA,QAAA,EAAA,MAAA,CAAA,UAAA,CAAA,QAAA,EAAA,MAAA,CAAA,UAAA,CAAA,QAAA;AAFA;AAAA,uBAGA,KAAA,CAAA,IAAA,CAAA,aAAA,EAAA,QAAA,CAHA;;AAAA;AAGA,gBAAA,EAHA;AAAA;AAAA,uBAIA,KAAA,CAAA,IAAA,CAAA,iBAAA,EAAA;AACA,kBAAA,QAAA,EAAA,MAAA,CAAA,QADA;AAEA,kBAAA,QAAA,EAAA,MAAA,CAAA,QAFA;AAGA,kBAAA,QAAA,EAAA,MAAA,CAAA,QAHA;AAIA,kBAAA,UAAA,EAAA,EAAA,CAAA,IAAA,CAAA;AAJA,iBAAA,CAJA;;AAAA;AAIA,gBAAA,EAJA;AAUA,gBAAA,MAAA,CAAA,SAAA,GAAA,EAAA,CAAA,IAAA;;AAVA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA,KAfA;AAgBA,IAAA,SAhBA,uBAgBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,KAAA,CAAA,GAAA,CAAA,iBAAA,CADA;;AAAA;AACA,gBAAA,QADA;AAEA,gBAAA,MAAA,CAAA,UAAA,GAAA,QAAA,CAAA,IAAA;AAFA,kDAGA,IAHA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA,KApBA;AAqBA,IAAA,YArBA,wBAqBA,MArBA,EAqBA;AACA,WAAA,QAAA,GAAA,EAAA;AACA,WAAA,UAAA,GAAA,MAAA;AACA,KAxBA;AAyBA,IAAA,YAzBA,wBAyBA,MAzBA,EAyBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,KAAA,CAAA,MAAA,CAAA,qBAAA,MAAA,CAAA,GAAA,CADA;;AAAA;AAEA,gBAAA,MAAA,CAAA,UAAA,GAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,SAAA;;AAHA,kDAIA,IAJA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA,KA9BA;AA+BA,IAAA,UA/BA,sBA+BA,MA/BA,EA+BA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,qBAAA,MAAA,CAAA,GAAA,EAAA;AACA,kBAAA,QAAA,EAAA,MAAA,CAAA,UAAA,CAAA,QADA;AAEA,kBAAA,QAAA,EAAA,MAAA,CAAA,UAAA,CAAA,QAFA;AAGA,kBAAA,QAAA,EAAA,MAAA,CAAA,UAAA,CAAA;AAHA,iBAAA,CAFA;;AAAA;AAOA,gBAAA,MAAA,CAAA,UAAA,GAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,SAAA;;AARA,kDASA,IATA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAUA;AAzCA;AAvBA,CAAA","sourcesContent":["<template>\n<div class=\"addFam\">\n\n<center>\n<div class=\"addSection\">\n\n  <div class=\"heading\">\n      <div class=\"box\">1</div>\n      <h1>Add Family Member</h1>\n  </div>\n\n  <div class=\"add\">\n      <div class=\"form\">\n        <textarea v-model=\"fullName\" placeholder=\"Full Name\"></textarea>\n        <p></p>\n        <textarea v-model=\"location\" placeholder=\"Current City\"></textarea>\n        <p></p>\n        <textarea v-model=\"birthday\" placeholder=\"Birthday (YYYY-MM-DD)\"></textarea>\n        <p></p>\n        <input type=\"file\" name=\"photo\" @change=\"fileChanged\">\n        <p></p>\n        <button @click=\"upload\">Upload</button>\n      </div>\n\n      <div class=\"upload\" v-if=\"addPerson\">\n        <h2>{{addPerson.fullName}}</h2>\n        <p>{{addPerson.location}}</p>\n        <p>{{addPerson.birthday}}</p>\n        <img :src=\"addPerson.peoplePath\" />\n      </div>\n    </div>\n</div>\n\n\n<div class=\"editSection\">\n\n    <div class=\"heading\">\n      <div class=\"box\">2</div>\n      <h1>Edit/Delete Family List</h1>\n    </div>\n    <div class=\"edit\">\n      <div class=\"form\">\n        <input v-model=\"findName\" placeholder=\"Search\">\n        <div class=\"suggestions\" v-if=\"suggestions.length > 0\">\n          <div class=\"suggestion\" v-for=\"s in suggestions\" :key=\"s.id\" @click=\"selectPerson(s)\">{{s.fullName}}\n          </div>\n        </div>\n      </div>\n\n\n      <div class=\"display\">\n      <center>\n        <div class=\"upload\" v-if=\"findPerson\">\n            <textarea v-model=\"findPerson.fullName\"></textarea>\n          <p></p>\n            <textarea v-model=\"findPerson.location\"></textarea>\n          <p></p>\n            <textarea v-model=\"findPerson.birthday\"></textarea>\n          <p></p>\n          <img :src=\"findPerson.peoplePath\" />\n        </div>\n      </center>\n      </div>\n      <div class=\"actions\" v-if=\"findPerson\">\n        <button @click=\"editPerson(findPerson)\">Edit</button>\n        <button @click=\"deletePerson(findPerson)\">Delete</button>\n      </div>\n    </div>\n\n\n</div>\n</center>\n\n</div>\n</template>\n\n\n\n\n\n\n<script>\nimport axios from 'axios';\nexport default {\n  name: 'People',\n  data() {\n    return {\n      fullName: \"\",\n      location: \"\",\n      birthday: \"\",\n      personFile: null,\n      addPerson: null,\n      peopleList: [],\n      findName: \"\",\n      findPerson: null,\n    }\n  },\n  computed: {\n     suggestions() {\n       let peopleList = this.peopleList.filter(person => person.fullName.toLowerCase().startsWith(this.findName.toLowerCase()));\n       return peopleList.sort((a, b) => a.fullName > b.fullName);\n     }\n   },\n  created() {\n    this.getPeople();\n  },\n  methods: {\n    fileChanged(event) {\n      this.personFile = event.target.files[0]\n    },\n    async upload() {\n        const formData = new FormData();\n        formData.append('photo', this.personFile, this.personFile.fullName, this.personFile.location, this.personFile.birthday)\n        let r1 = await axios.post('/api/photos', formData);\n        let r2 = await axios.post('/api/peopleList', {\n          fullName: this.fullName,\n          location: this.location,\n          birthday: this.birthday,\n          peoplePath: r1.data.peoplePath\n        });\n        this.addPerson = r2.data;\n    },\n    async getPeople() {\n        let response = await axios.get(\"/api/peopleList\");\n        this.peopleList = response.data;\n        return true;\n    },\n    selectPerson(person) {\n      this.findName = \"\";\n      this.findPerson = person;\n   },\n   async deletePerson(person) {\n       await axios.delete(\"/api/peopleList/\" + person._id);\n       this.findPerson = null;\n       this.getPeople();\n       return true;\n   },\n   async editPerson(person) {\n\n        await axios.put(\"/api/peopleList/\" + person._id, {\n          fullName: this.findPerson.fullName,\n          birthday: this.findPerson.birthday,\n          location: this.findPerson.location,\n        });\n        this.findPerson = null;\n        this.getPeople();\n        return true;\n    },\n  },\n}\n</script>\n\n\n\n\n\n\n\n\n\n<style scoped>\n.image h1 {\n  font-style: italic;\n  font-size: 1em;\n}\n\n.image p {\n  font-size: .5em;\n}\n\n.heading {\n  display: flex;\n  margin-bottom: 20px;\n  margin-top: 20px;\n}\n\n.heading h1 {\n  margin-top: 8px;\n  margin-left: 10px;\n}\n\n.add,\n.edit, {\n  display: flex;\n  justify-content: center;\n  align-content:center;\n}\n\n.display{\n  padding: 10px;\n}\n\n.addSection,\n.editSection {\n  background: #1c3738;\n  margin: 60px;\n  padding: 30px;\n  color: #fff;\n  width: 50%;\n}\n\n.box {\n  width: 18px;\n  height: 18px;\n  padding: 8px;\n  background: #84a9c0;\n  color: #fff;\n  text-align: center;\n}\n\n\n/* Form */\ninput,\ntextarea,\nselect,\nbutton,\n.description {\n  font-family: 'Montserrat', sans-serif;\n  font-size: 1em;\n}\n\n.description {\n  font-size: .5em;\n}\n\n.form {\n  margin-right: 50px;\n}\n\n/* Uploaded images */\n.upload h2 ,\n.upload p {\n  margin: 0px;\n}\n\n.upload img {\n  max-width: 300px;\n}\n\n\n\n\n/* Suggestions */\n.suggestions {\n  width: 200px;\n  border: 1px solid #ccc;\n}\n\n.suggestion {\n  min-height: 20px;\n}\n\n.suggestion:hover {\n  background-color: #5BDEFF;\n  color: #fff;\n}\n\n\n\n\n\n\n\n\n</style>\n"],"sourceRoot":"src/views"}]}